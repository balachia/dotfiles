# color contrast play
set -g status-style fg=default,bg=default
set -g window-status-style fg=colour8,bg=default,bold
set -g window-status-current-style fg=colour9,bg=default

set -g pane-border-style fg=colour8,bg=default
set -g pane-active-border-style fg=colour1,bg=default
set -g display-panes-colour colour8
set -g display-panes-active-colour colour9

