# blanked
set -g status-style fg=default,bg=default
set -g window-status-style fg=colour0,bg=default,bold
set -g window-status-current-style fg=colour1,bg=default

set -g pane-border-style fg=colour0,bg=default
set -g pane-active-border-style fg=colour1,bg=default
set -g display-panes-colour colour0
set -g display-panes-active-colour colour1

